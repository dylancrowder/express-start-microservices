{
  "name": "express-start-microservices",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "auth",
    "products",
    "gateway",
    "common"
  ],
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/swagger-ui-express": "^4.1.8",
    "turbo": "^2.5.5"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start:auth": "turbo run dev --filter=auth",
    "start:products": "turbo run dev --filter=products",
    "start:gateway": "turbo run dev --filter=gateway"
  },
  "packageManager": "npm@9.8.0",
  "dependencies": {
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  }
}
